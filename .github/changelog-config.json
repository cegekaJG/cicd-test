{
    "categories": [
      {
        "title": "## üöÄ Enhancements",
        "labels": ["enhancement"]
      },
      {
        "title": "## üêõ Fixes",
        "labels": ["bug"]
      },
      {
        "title": "## üìñ Documentation",
        "labels": ["documentation"]
      }
    ],
    "ignore_labels": [
      "ignore"
    ],
    "sort": {
      "order": "ASC",
      "on_property": "mergedAt"
    },
    "template": "${{CHANGELOG}}\n\nUnlabeled\n\n${{UNCATEGORIZED}}",
    "pr_template": "- ${{TITLE}}\n   - PR: <${{URL}}> by ${{ASSIGNEES}}",
    "empty_template": "- no changes",
    "transformers": [
      {
        "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
        "target": "- $4\n  - $6"
      }
    ],
    "trim_values": false
}
